{
  "name": "cartalia",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "deploy": "npm run build && vercel --prod",
    "deploy:preview": "npm run build && vercel",
    "analyze": "vite build --mode analyze",
    "lighthouse": "lighthouse http://localhost:4173 --output html --output-path ./lighthouse-report.html"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@vee-validate/zod": "^4.15.1",
    "axios": "^1.10.0",
    "pinia": "^3.0.3",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "vuetify": "^3.9.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^24.0.14",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "jsdom": "^26.1.0",
    "sass": "^1.89.2",
    "typescript": "~5.8.3",
    "vite": "^5.0.12",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  }
}
